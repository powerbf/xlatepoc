# Orc

# Since Orc is masculine it takes der in nominative
#msgctxt "nominative"
msgid "the orc"
msgstr "der Ork"

# Since Orc is masculine it takes den in accusitive
msgctxt "accusative"
msgid "the orc"
msgstr "den Ork"

# Since Orc is masculine it takes dem in dative.
msgctxt "dative"
msgid "the orc"
msgstr "dem Ork"

#msgctxt "nominative"
msgid "an orc"
msgstr "ein Ork"

msgctxt "accusative"
msgid "an orc"
msgstr "einen Ork"

msgctxt "dative"
msgid "an orc"
msgstr "einem Ork"

# Elf

# Since Elf is feminine it takes die in nominative.
#msgctxt "nominative"
msgid "the elf"
msgstr "die Elfe"

# Since Elf is feminine it takes die in accusative.
msgctxt "accusative"
msgid "the elf"
msgstr "die Elfe"

# Since Elf is feminine it takes der in dative case.
msgctxt "dative"
msgid "the elf"
msgstr "der Elfe"

#msgctxt "nominative"
msgid "an elf"
msgstr "eine Elfe"

msgctxt "accusative"
msgid "an elf"
msgstr "eine Elfe"

msgctxt "dative"
msgid "an elf"
msgstr "einer Elfe"

# Natasha

# is the same regardless of case
msgid "Natasha"
msgstr "Natascha"






